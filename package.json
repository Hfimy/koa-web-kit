{
  "name": "koa-web-kit",
  "version": "2.4.2",
  "description": "A modern, production-ready, and full-stack node web framework",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env ENABLE_HMR=1 ENABLE_SSR=0 node app.js",
    "dev:ssr": "cross-env ENABLE_HMR=0 ENABLE_SSR=1 npm-run-all -p watch watch:ssr start",
    "dev:watch": "cross-env ENABLE_HMR=0 ENABLE_SSR=0 npm-run-all -p watch start",
    "start": "nodemon app.js",
    "deploy": "./deploy.sh",
    "deploy:noinstall": "npm run deploy -- 1",
    "build:dev": "webpack --progress --hide-modules --config config/webpack.config.dev.js",
    "build": "npm run prod",
    "ssg": "cross-env STATIC_PREFIX='' APP_PREFIX='' npm run prod",
    "build:analyze": "npm run prod:analyze",
    "watch": "webpack --watch --progress --hide-modules --config config/webpack.config.dev.js",
    "node": "webpack --progress --config config/webpack.config.node.js",
    "watch:ssr": "npm run ssr -- --watch",
    "ssr": "webpack --progress --config config/webpack.config.ssr.js",
    "prod": "cross-env NODE_ENV=production webpack -p --progress --hide-modules --config config/webpack.config.prod.js",
    "prod:analyze": "cross-env NODE_ENV=production BUNDLE_ANALYZER=true webpack -p --progress --hide-modules --config config/webpack.config.prod.js",
    "analyze": "npm run prod:analyze",
    "test": "npm run build:dev",
    "precommit": "lint-staged"
  },
  "engines": {
    "node": ">= 8"
  },
  "lint-staged": {
    "**/*.{js,jsx,json,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JasonBoy/koa-web-kit.git"
  },
  "keywords": [
    "koa",
    "es6",
    "webpack",
    "react",
    "bootstrap",
    "fullstack",
    "framework"
  ],
  "author": "jasonlikenfs@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JasonBoy/koa-web-kit/issues"
  },
  "homepage": "https://github.com/JasonBoy/koa-web-kit#readme",
  "readme": "README.md",
  "dependencies": {
    "bluebird": "3.5.1",
    "bootstrap": "4.1.1",
    "chalk": "2.4.1",
    "classnames": "2.2.6",
    "from2-string": "1.1.0",
    "get-port": "3.2.0",
    "koa": "2.5.1",
    "koa-body": "4.0.3",
    "koa-compress": "3.0.0",
    "koa-convert": "1.2.0",
    "koa-favicon": "2.0.1",
    "koa-helmet": "4.0.0",
    "koa-history-api-fallback": "0.1.3",
    "koa-morgan": "1.0.1",
    "koa-mount": "3.0.0",
    "koa-router": "7.4.0",
    "koa-session": "5.8.1",
    "koa-static": "5.0.0",
    "lodash": "4.17.10",
    "lodash.isempty": "4.4.0",
    "make-dir": "1.3.0",
    "moment": "2.22.2",
    "pm2": "2.10.4",
    "prop-types": "15.6.2",
    "qs": "6.5.2",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-loadable": "5.4.0",
    "react-router-dom": "4.3.1",
    "request": "2.87.0",
    "request-debug": "0.2.0",
    "url-parse": "1.4.1",
    "whatwg-fetch": "2.0.4",
    "winston": "3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "latest",
    "@babel/core": "latest",
    "@babel/helper-module-imports": "7.0.0-beta.51",
    "@babel/plugin-proposal-class-properties": "latest",
    "@babel/plugin-proposal-object-rest-spread": "latest",
    "@babel/plugin-syntax-dynamic-import": "latest",
    "@babel/plugin-transform-react-jsx-source": "latest",
    "@babel/plugin-transform-runtime": "latest",
    "@babel/preset-env": "latest",
    "@babel/preset-react": "latest",
    "autoprefixer": "8.6.3",
    "babel-loader": "8.0.0-beta.4",
    "babel-minify-webpack-plugin": "0.3.1",
    "babel-plugin-import-inspector": "nodkz/babel-plugin-import-inspector.git#babel7",
    "babel-plugin-lodash": "3.3.4",
    "chai": "4.1.2",
    "clean-webpack-plugin": "0.1.19",
    "copy-webpack-plugin": "4.5.1",
    "cross-env": "5.2.0",
    "css-loader": "0.28.11",
    "del": "3.0.0",
    "file-loader": "1.1.11",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "husky": "0.14.3",
    "import-inspector": "2.0.0",
    "koa-webpack": "5.0.2",
    "lint-staged": "7.2.0",
    "mini-css-extract-plugin": "0.4.0",
    "mocha": "5.2.0",
    "moment-locales-webpack-plugin": "1.0.7",
    "node-sass": "4.9.0",
    "nodemon": "1.17.5",
    "npm-run-all": "4.1.3",
    "optimize-css-assets-webpack-plugin": "4.0.2",
    "postcss-loader": "2.1.5",
    "preload-webpack-plugin": "3.0.0-beta.1",
    "prettier": "1.13.5",
    "sass-loader": "7.0.3",
    "style-loader": "0.21.0",
    "uglifyjs-webpack-plugin": "1.2.6",
    "url-loader": "1.0.1",
    "webpack": "4.12.0",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-cli": "3.0.8",
    "webpack-manifest-plugin": "2.0.3",
    "webpack-merge": "4.1.3",
    "webpack-node-externals": "1.7.2"
  },
  "browserslist": [
    "> 0.5%",
    "not ie 11",
    "not op_mini all"
  ],
  "nodemonConfig": {
    "watch": [
      "api/",
      "config/",
      "build/node",
      "routes/",
      "utils/",
      "services/"
    ]
  }
}
